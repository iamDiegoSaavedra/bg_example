	</div>
</div>


{% endblock %} {% block javascripts %} {{ parent() }}
<script type="text/javascript"
	src="{{ asset('bundles/sistemagestion/js/prueba.js')}}"></script>
<script type="text/javascript"
	src="{{ asset('bundles/sistemagestion/plugin/all.js')}}"></script>
